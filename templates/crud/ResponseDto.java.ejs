package <%= packageName %>.<%= moduleName %>.application.dtos;

<% if (hasValueObjects) { %>
import <%= packageName %>.<%= moduleName %>.domain.models.valueObjects.*;
<% } %>
<% if (hasEnums) { %>
import <%= packageName %>.<%= moduleName %>.domain.models.enums.*;
<% } %>
<% if (relationships && relationships.length > 0) { %>
import java.util.List;
<% } %>
<% imports.forEach(imp => { %>
<%- imp %>
<% }); %>

public record <%= aggregateName %>ResponseDto(
<% allFields.forEach((field, idx) => { %>
    <%- field.javaType %> <%= field.name %><% if (idx < allFields.length - 1 || relationships.length > 0) { %>,<% } %>
<% }); %>
<% relationships.forEach((rel, idx) => { %>
  <% if (rel.type === 'OneToMany') { %>
    List<<%= rel.target %>Dto> <%= rel.fieldName %><% if (idx < relationships.length - 1) { %>,<% } %>
  <% } else if (rel.type === 'OneToOne') { %>
    <%= rel.target %>Dto <%= rel.fieldName %><% if (idx < relationships.length - 1) { %>,<% } %>
  <% } %>
<% }); %>
) {
}
