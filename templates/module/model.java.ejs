import <%= packageName %>.shared.enums.Status;
import jakarta.persistence.*;
import lombok.*;

/**
 * <%= className %> entity
 */
@Entity
@Table(name = "<%= tableName %>")
@Getter
@Setter
@NoArgsConstructor
@AllArgsConstructor
@Builder
public class <%= className %> extends <%= baseEntityClass %><Long> {
    
