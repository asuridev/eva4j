server:
  port: <%= serverPort %>
  servlet:
    context-path: /api/v1

springdoc:
  swagger-ui:
    path: /swagger-ui.html
    enabled: true
  api-docs:
    path: /api-docs

management:
  endpoint:
    health:
      probes:
        enabled: true
      show-details: always
  endpoints:
    web:
      exposure:
        include: "health"

spring:
  threads:
    virtual:
      enabled: true
  application:
    name: <%= artifactId %>
  profiles:
    active: ${PROFILE:develop}
