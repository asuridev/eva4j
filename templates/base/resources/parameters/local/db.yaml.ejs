<% if (dependencies.includes('data-jpa')) { %>spring:
  datasource:
    url: <%= databaseUrl %>
    username: <%= databaseUsername %>
    password: <%= databasePassword %>
    driver-class-name: <%= databaseDriverClass %>
  
  jpa:
    hibernate:
      ddl-auto: update
    show-sql: true
    properties:
      hibernate:
        format_sql: true
        dialect: <%= hibernateDialect %>

logging:
  level:
    root: INFO
    <%= packageName %>: DEBUG
<% } %>
